                    x:Name="BackgroundEllipse"
                    UseLayoutRounding="False"
                    Width="40"
                    Height="40"
                    Fill="Aqua"/>
                                    <Ellipse
                    x:Name="OutlineEllipse"
                    UseLayoutRounding="False"
